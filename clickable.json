{
    "builder": "custom",
    "default": "build",
    "kill": "pvms",
    "dependencies_ppa": [
        "ppa:deadsnakes/ppa",
        "\"deb [arch=amd64] http://archive.ubuntu.com/ubuntu xenial-backports universe restricted multiverse main\"",
        "\"deb [arch=arm64,armhf] http://ports.ubuntu.com/ubuntu-ports xenial-backports universe restricted multiverse main\""
    ],
    "dependencies_target": [
        "libgbm-dev",
        "libdrm-dev",
        "libepoxy-dev",
        "libdbus-1-dev",
        "libasound2-dev",
        "libsndio-dev",
        "libxcursor-dev",
        "libxinerama-dev",
        "libxi-dev",
        "libxrandr-dev",
        "libxss-dev",
        "libjpeg-dev",
        "mesa-common-dev",
        "autoconf-archive:amd64",
        "bison",
        "flex",
        "gettext",
        "python3.6",
        "python3-six:any",
        "python3-pyparsing:any",
        "python-six:any",
        "python-pyparsing:any",
        "meson:any",
        "ninja-build:any/xenial-backports",
        "libtool",
        "libaio-dev",
        "libseccomp-dev",
        "libpulse-dev",
        "libpixman-1-dev",
        "libgles2-mesa-dev",
        "libegl1-mesa-dev",
        "zlib1g-dev"
    ],
    "install_lib": [
        "/lib/${ARCH_TRIPLET}/libaio.so*"
    ],
    "build": "${ROOT}/build.sh",
    "make_jobs": 6,
    "qt_version": "5.12"
}
